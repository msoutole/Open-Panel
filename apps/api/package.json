{
  "name": "@openpanel/api",
  "version": "1.0.0",
  "private": true,
  "description": "OpenPanel API Backend",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "test": "cross-env NODE_ENV=test DATABASE_URL=\"postgresql://test:test@localhost:5432/test_db\" vitest",
    "test:watch": "vitest --watch",
    "db:generate": "npx prisma generate --config ../../prisma.config.mjs",
    "db:migrate": "npx prisma migrate dev --config ../../prisma.config.mjs",
    "db:push": "npx prisma db push --config ../../prisma.config.mjs",
    "db:studio": "npx prisma studio --config ../../prisma.config.mjs",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.5",
    "@hono/zod-validator": "^0.7.4",
    "@openpanel/shared": "*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/client": "^7.1.0",
    "@types/ws": "^8.18.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.63.0",
    "dockerode": "^4.0.9",
    "dotenv": "^16.6.1",
    "hono": "^4.10.4",
    "hono-rate-limiter": "^0.4.2",
    "ioredis": "^5.8.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.10.0",
    "mysql2": "^3.11.5",
    "otplib": "^12.0.1",
    "pg": "^8.13.1",
    "qrcode": "^1.5.4",
    "redis": "^5.9.0",
    "winston": "^3.18.3",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@prisma/config": "^7.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dockerode": "^3.3.46",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.14.0",
    "@types/qrcode": "^1.5.5",
    "prisma": "^7.1.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vitest": "^4.0.8"
  }
}
